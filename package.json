{
    "description": "BunqDesktop application",
    "name": "bunq-desktop",
    "productName": "BunqDesktop",
    "version": "0.3.1",
    "author": "Gregory Goijaerts (crecketgaming@gmail.com)",
    "license": "MIT",
    "main": "app/background.js",
    "build": {
        "appId": "com.bunq.desktop",
        "files": [
            "app/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "publish": null
    },
    "scripts": {
        "webpack": "cross-env NODE_ENV=production webpack --progress --colors",
        "webpack:dev": "cross-env NODE_ENV=development webpack --debug --watch --progress --colors",
        "postinstall": "install-app-deps",
        "build": "gulp build",
        "prerelease": "gulp build --env=production && yarn run webpack",
        "release": "build",
        "start": "gulp start --env=development",
        "pretest": "gulp build-unit --env=test",
        "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
        "coverage": "yarn test -- -R scripts/istanbul-reporter",
        "pree2e": "gulp build-e2e --env=test",
        "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
    },
    "dependencies": {
        "electron-localshortcut": "^2.0.2",
        "fs-jetpack": "^1.0.0",
        "mousetrap": "^1.6.1"
    },
    "devDependencies": {
        "@bunq-community/bunq-js-client": "^0.2.0",
        "@types/axios": "^0.14.0",
        "awaiting": "^2.0.1",
        "axios": "^0.16.2",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-react-html-attrs": "^2.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-system-import-transformer": "^3.1.0",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-inline-elements": "^6.22.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chai": "^4.1.0",
        "clean-webpack-plugin": "^0.1.17",
        "cross-env": "^4.0.0",
        "css-loader": "^0.28.7",
        "electron": "^1.6.11",
        "electron-builder": "^19.16.0",
        "electron-mocha": "^4.0.0",
        "es6-promise-promise": "^1.0.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-batch": "^1.0.5",
        "gulp-less": "^3.3.2",
        "gulp-plumber": "^1.1.0",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^4.3.11",
        "html-webpack-plugin": "^2.26.0",
        "istanbul": "^0.4.5",
        "localforage": "^1.5.2",
        "loglevel": "^1.4.1",
        "material-ui": "1.0.0-beta.12",
        "material-ui-icons": "^1.0.0-beta.10",
        "minimist": "^1.2.0",
        "mocha": "^3.4.2",
        "node-forge": "^0.7.1",
        "node-sass": "^4.5.3",
        "prop-types": "^15.5.8",
        "react": "^16.0.0",
        "react-dom": "16.0.0",
        "react-helmet": "^5.0.2",
        "react-loadable": "^3.2.2",
        "react-number-format": "^3.0.0-alpha2",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.0",
        "redux": "^3.6.0",
        "redux-logger": "^3.0.1",
        "redux-mock-store": "^1.2.3",
        "redux-thunk": "^2.1.0",
        "rollup": "^0.45.2",
        "rollup-plugin-istanbul": "^1.1.0",
        "sass-loader": "^6.0.6",
        "source-map-support": "^0.4.15",
        "spectron": "^3.7.2",
        "store": "^2.0.4",
        "style-loader": "^0.16.1",
        "ts-loader": "^2.3.7",
        "typescript": "^2.5.3",
        "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
        "webpack": "^3.7.1"
    }
}
