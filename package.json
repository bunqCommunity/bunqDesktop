{
    "name": "bunqdesktop",
    "description": "The unofficial, free and open source desktop application for the bunq API",
    "productName": "bunqDesktop",
    "homepage": "https://bunqdesk.top/",
    "version": "0.8.11",
    "author": {
        "name": "bunqCommunity",
        "email": "bunqcommunity@gmail.com"
    },
    "license": "MIT",
    "main": "app/background.js",
    "supported_languages": [
        "en",
        "nl",
        "de"
    ],
    "scripts": {
        "webpack": "cross-env NODE_ENV=production webpack --progress --colors",
        "webpack:dev-original": "cross-env NODE_ENV=development webpack --debug --watch --progress --colors",
        "webpack:dev": "cross-env NODE_ENV=development node --max_old_space_size=4092 ./node_modules/webpack/bin/webpack.js --debug --watch --progress --colors",
        "prerelease": "npm run update-release-files && gulp build --env=production && yarn run webpack",
        "release": "build",
        "release:appx": "build -w appx",
        "start": "gulp start --env=development",
        "start:dev": "cross-env NODE_ENV=development gulp start --env=development",
        "test": "cross-env JEST=true jest --coverage --verbose",
        "test:dev": "cross-env JEST=true jest --watch --coverage",
        "postinstall": "install-app-deps",
        "build": "gulp build",
        "dist": "build",
        "update-icons": "node tasks/generate_icons.js",
        "update-translations": "node tasks/update_translations.js",
        "update-release-files": "node tasks/update_release_files.js"
    },
    "build": {
        "appId": "com.bunqdesktop",
        "files": [
            "app/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "linux": {
            "category": "Office",
            "target": [
                {
                    "target": "AppImage",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                },
                {
                    "target": "deb",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                },
                {
                    "target": "snap",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "appx": {
            "applicationId": "bunqDesktop",
            "identityName": "48566bunqCommunity.bunqDesktop",
            "languages": [
                "en-US",
                "nl",
                "de"
            ],
            "publisherDisplayName": "bunqCommunity",
            "publisher": "CN=3407C75B-E8E9-4429-AE58-DECF35DAB82D"
        },
        "mac": {
            "category": "public.app-category.finance"
        },
        "publish": null
    },
    "publish": null,
    "jest": {
        "globals": {
            "ts-jest": {
                "useBabelrc": true
            }
        },
        "modulePathIgnorePatterns": [
            "/app",
            "/dist"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/public/",
            "/dist/",
            ".html"
        ],
        "transform": {
            "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "dependencies": {
        "@sentry/electron": "^0.5.5",
        "electron-localshortcut": "^2.0.2",
        "electron-log": "^2.2.9",
        "electron-settings": "^3.1.3",
        "fs-jetpack": "^1.0.0",
        "glob": "^7.1.2",
        "i18next": "^10.6.0",
        "i18next-xhr-backend": "^1.5.1",
        "upath": "1.0.5"
    },
    "devDependencies": {
        "@bunq-community/bunq-js-client": "^0.22.5",
        "@bunq-community/bunqdesktop-templates": "^1.2.0",
        "@fortawesome/fontawesome-free": "^5.2.0",
        "@google-cloud/translate": "^1.1.0",
        "@material-ui/core": "^1.4.3",
        "@material-ui/icons": "^1",
        "@material-ui/lab": "^1.0.0-alpha.9",
        "@types/axios": "^0.14.0",
        "@types/jest": "^22.1.3",
        "@types/moxios": "^0.4.8",
        "@types/node": "^9.4.6",
        "@types/react-i18next": "^7.3.2",
        "animate.css": "^3.6.1",
        "awaiting": "^2.0.1",
        "axios": "^0.18.0",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-dynamic-import-node": "^2.0.0",
        "babel-plugin-react-html-attrs": "^2.1.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-system-import-transformer": "^3.1.0",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-inline-elements": "^6.22.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chart.js": "^2.7.2",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.2.0",
        "css-loader": "^0.28.11",
        "date-fns": "2.0.0-alpha.7",
        "electron": "^2.0.8",
        "electron-builder": "^20.28.2",
        "electron-reload": "^1.2.5",
        "email-validator": "^1.1.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "google-libphonenumber": "^3.1.3",
        "gulp": "^3.9.1",
        "gulp-batch": "^1.0.5",
        "gulp-less": "^3.5.0",
        "gulp-plumber": "^1.2.0",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^5.0.0",
        "html-webpack-plugin": "^3.2.0",
        "iban": "^0.0.10",
        "increase-memory-limit": "^1.0.6",
        "jest": "^23.2.0",
        "localforage": "^1.7.2",
        "loglevel": "^1.6.1",
        "material-ui-pickers": "^1.0.0-rc.10",
        "mini-css-extract-plugin": "^0.4.0",
        "minimist": "^1.2.0",
        "mocha": "^5.2.0",
        "natives": "^1.1.4",
        "node-forge": "^0.7.5",
        "node-sass": "^4.9.0",
        "prop-types": "^15.6.2",
        "qrcode.react": "^0.8.0",
        "react": "^16.4.2",
        "react-chartjs-2": "^2.7.2",
        "react-color": "^2.14.1",
        "react-copy-to-clipboard": "^5.0.1",
        "react-countdown-now": "^1.3.0",
        "react-dom": "^16.4.1",
        "react-helmet": "^5.2.0",
